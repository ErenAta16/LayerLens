{
  "include": [
    "layerlens",
    "scripts",
    "demos",
    "experiments",
    "tests"
  ],
  "exclude": [
    "**/__pycache__",
    "**/node_modules",
    "**/.git",
    "**/build",
    "**/dist",
    "**/*.egg-info",
    "layerlens.egg-info"
  ],
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.9",
  "pythonPlatform": "All",
  "typeCheckingMode": "basic",
  "venvPath": ".",
  "venv": ".",
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.9",
      "pythonPlatform": "All",
      "extraPaths": [
        "."
      ]
    }
  ]
}

